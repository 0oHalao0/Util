@model ApplicationDto

<util-form id="form" on-submit="submit(form)">
    <util-layout flex="70%" x-align="Center">
        <util-card>
            <util-card-header>
                编辑应用程序
            </util-card-header>
            <util-card-content class="formContent">
                <util-layout flex="100%" x-align="Center">
                    <util-textbox for="Code"></util-textbox>
                </util-layout>
                <util-layout flex="100%" x-align="Center">
                    <util-textbox for="Name"></util-textbox>
                </util-layout>
                <util-layout flex="100%" x-align="Center">
                    <util-textbox for="Comment"></util-textbox>
                </util-layout>
                <util-layout class="mt30">
                    <util-slide-toggle for="Enabled"></util-slide-toggle>
                </util-layout>
                <util-layout class="mt30">
                    <util-slide-toggle for="RegisterEnabled"></util-slide-toggle>
                </util-layout>
                <util-layout gap="20px">
                    <util-layout-item>
                        <util-textbox placeholder="输入框1"></util-textbox>
                    </util-layout-item>
                    <util-layout-item>
                        <util-textbox placeholder="输入框2"></util-textbox>
                    </util-layout-item>
                    <util-layout-item>
                        <util-textbox placeholder="输入框3"></util-textbox>
                    </util-layout-item>
                </util-layout>
                <util-layout gap="20px" class="mt30" flex="100%">
                    <util-datepicker for="LastModificationTime"></util-datepicker>
                </util-layout>
                <util-layout class="mt30" flex="100%">
                    <util-select for="Version">
                        <util-select-list name="1">
                            <util-select-list-item value="1">1</util-select-list-item>
                        </util-select-list>
                    </util-select>
                </util-layout>
                <util-layout class="mt30" flex="100%">
                    <util-textarea placeholder="多行文本" min-rows="3"></util-textarea>
                </util-layout>
            </util-card-content>
            <util-card-actions align="Right">
                <util-button text="返回" styles="Default" on-click="back()"></util-button>
                <util-button type="Submit" color="Primary"></util-button>
            </util-card-actions>
        </util-card>
    </util-layout>
</util-form>
