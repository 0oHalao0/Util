{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": true,
    "lib": [ "es2017", "dom" ],
    "outDir": "wwwroot/dist/",
    "declaration": false,
    "typeRoots": [ "node_modules/@types" ]
  },
  "exclude": [ "node_modules" ]
}
