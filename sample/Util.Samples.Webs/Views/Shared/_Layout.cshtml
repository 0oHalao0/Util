<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    <base href="/" />
    <link rel="icon" type="image/x-icon" href="favicon.ico">
    <link rel="stylesheet" type="text/css" href="/dist/vendor.css" asp-append-version="true">
    <link rel="stylesheet" type="text/css" href="/dist/app.css" asp-append-version="true">
</head>
<body>
    @RenderBody()
    <environment include="Development">
        <script src="/dist/polyfills.js" asp-append-version="true"></script>
        <script src="/dist/vendor.js" asp-append-version="true"></script>
        <script src="/dist/util.js" asp-append-version="true"></script>
        <script src="/dist/app.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="/dist/app.js" asp-append-version="true"></script>
    </environment>
    @RenderSection( "Scripts", required: false )
</body>
</html>
